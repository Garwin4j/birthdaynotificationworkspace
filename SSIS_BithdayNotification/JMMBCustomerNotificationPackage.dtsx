<property id="1252" name="SourceCode" dataType="System.String" state="cdata" isArray="true" description="Stores the source code of the component" typeConverter="" UITypeEditor="" containsID="false" expressionType="None"><arrayElements arrayElementCount="18"><arrayElement dataType="System.String"><![CDATA[\main.cs]]></arrayElement><arrayElement dataType="System.String"><![CDATA[/* Microsoft SQL Server Integration Services Script Component
*  Write scripts using Microsoft Visual C# 2008.
*  ScriptMain is the entry point class of the script.*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.IO;
using System.Text;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void PreExecute()
    {
        base.PreExecute();
        /*
          Add your code here for preprocessing or remove if not needed
        */
    }

    public override void PostExecute()
    {
        base.PostExecute();
        /*
          Add your code here for postprocessing or remove if not needed
          You can set read/write variables here, for example:
          Variables.MyIntVar = 100
        */
    }

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        string template = Variables.NotificationEmailTemplate;
       
        if (String.IsNullOrEmpty(template))
        {
            Boolean canceled = true;
            this.ComponentMetaData.FireError(-1, null, "Email Notification Template is empty", null, 1, out canceled);
            return;
        }

        do
        {
            StringBuilder message = new StringBuilder(template);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum1" + Variables.TemplateEndDelimiter, Row.Coloum1);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum2" + Variables.TemplateEndDelimiter, Row.Coloum2);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum3" + Variables.TemplateEndDelimiter, Row.Coloum3);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum4" + Variables.TemplateEndDelimiter, Row.Coloum4);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum5" + Variables.TemplateEndDelimiter, Row.Coloum5);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum6" + Variables.TemplateEndDelimiter, Row.Coloum6);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum7" + Variables.TemplateEndDelimiter, Row.Coloum7);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum8" + Variables.TemplateEndDelimiter, Row.Coloum8);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum9" + Variables.TemplateEndDelimiter, Row.Coloum9);
            message = message.Replace(Variables.TemplateStartingDelimiter + "Coloum10" + Variables.TemplateEndDelimiter, Row.Coloum10);
            Row.EmailMessage = message.ToString();
        } while (Row.NextRow());
    }

}
